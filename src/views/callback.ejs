<body>

</body>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script>
    const status = "<%=status%>"; // ok, error
    console.log(status)
    if (status === "ok") {
        Swal.fire({
            title: "<%=title%>",
            text: "<%=text%>",
            icon: "success"
        });
    } else {
        Swal.fire({
            icon: "error",
            title: "<%=title%>",
            text: "<%=text%>"
        });
    }
</script>

</html>